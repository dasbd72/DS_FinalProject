{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "Build_workspaceFolder",
      "command": "g++",
      "args": [
        "-std=c++17",
        "-o",
        "'${workspaceFolder}\\essay-search.exe'",
        "-g",
        "'${workspaceFolder}\\src\\*.cpp'",
        "-lstdc++fs"
      ],
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
